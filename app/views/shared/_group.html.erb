<%= simple_form_for [@resort, @group] do |f| %>
  <%= f.input :name, as: :string%>
  <%= f.input :description, as: :text %>
  <%= f.input :locked %>
  <%= f.input :photo, as: :file %>
  <div class="d-flex justify-content-center">
    <%= f.submit "Submit", class: "button-book"  %>
  </div>
<% end %>
