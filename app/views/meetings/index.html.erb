<% @meetings.each do |meeting| %>
    <ul>
      <li><%= meeting.address %></li>
      <li><%= meeting.meeting_date %></li>
      <h2><%= link_to "Edit", edit_meeting_path(meeting) %></h2>
      <h2><%= link_to "Delete", meeting_path(meeting), method: :delete, data: { confirm: "Are you sure?" } %></h2>
    </ul>
<% end %>

<div
  id="map"
  style="width: 100%;
  height: 600px;"
  data-markers="<%= @markers.to_json %>"
  data-mapbox-api-key="<%= ENV['MAPBOX_API_KEY'] %>"
></div>
